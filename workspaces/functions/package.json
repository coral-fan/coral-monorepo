{
  "name": "functions",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint ./src",
    "webpack": "webpack --progress --color --stats-error-details",
    "build": "yarn webpack --mode=production",
    "build:dev": "yarn webpack --mode=development",
    "start": "yarn build:dev && yarn start:functions",
    "start:functions": "firebase emulators:start",
    "shell": "yarn build && firebase functions:shell",
    "serve": "yarn shell",
    "deploy": "firebase deploy --only functions:nonce,functions:auth,functions:isSigningUp",
    "logs": "firebase functions:log",
    "clean": "rm -rf node_modules"
  },
  "main": "dist/index.js",
  "dependencies": {
    "@ethersproject/address": "^5.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0"
  },
  "private": true
}
