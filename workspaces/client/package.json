{
  "name": "client",
  "version": "0.0.0",
  "private": true,
  "main": "next-server.js",
  "scripts": {
    "start": "nodemon --watch next.config.js --exec 'next dev'",
    "start:functions": "next build:development && firebase -c firebase.development.json emulators:start",
    "start:mobile-testing": "next build:mobile-testing && firebase -c firebase.development.json emulators:start",
    "start:debug": "NODE_OPTIONS='--inspect' next dev",
    "deploy": "firebase deploy --only functions:nextServer,hosting",
    "build": "next build",
    "build:development": "env-cmd -f .env. next build",
    "build:mobile-testing": "env-cmd -f .env.mobile-testing next build",
    "serve": "next start",
    "lint": "next lint",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@ethersproject/address": "^5.4.0",
    "@ethersproject/providers": "^5.4.5",
    "@ethersproject/transactions": "^5.4.0",
    "@ethersproject/wallet": "^5.4.0",
    "@react-rxjs/core": "^0.8.3",
    "@react-rxjs/utils": "^0.9.2",
    "@reduxjs/toolkit": "^1.6.2",
    "@toruslabs/openlogin": "^1.2.0",
    "@web3-react/abstract-connector": "^6.0.7",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/types": "^6.0.7",
    "axios": "^0.21.4",
    "firebase": "^9.1.0",
    "firebase-admin": "9.11.1",
    "firebase-functions": "^3.16.0",
    "next": "^12.0.4",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.3.0",
    "theme-in-css": "^0.2.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@types/react": "^17.0.25",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "env-cmd": "^10.1.0",
    "eslint-config-next": "^11.1.2",
    "nodemon": "^2.0.12"
  }
}
