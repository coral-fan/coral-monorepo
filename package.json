{
  "name": "coral-monorepo",
  "private": true,
  "scripts": {
    "app": "yarn workspace app",
    "clean": "yarn workspaces run clean && rm -rf node_modules",
    "patch-package": "patch-package",
    "start:tunnel": "ngrok start --all --config ngrok.yaml",
    "postinstall": "patch-package && yarn workspaces run postinstall"
  },
  "workspaces": [
    "workspaces/*"
  ],
  "devDependencies": {
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "typescript": "^4.5.5"
  }
}
